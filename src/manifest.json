{
  "manifest_version": 3,
  "name": "NetSuite Field Explorer",
  "description": "Examine the fields in a Netsuite record",
  "author": "Michoel Chaikin <micholi@gmail.com>",
  "homepage_url": "https://github.com/michoelchaikin/netsuite-field-explorer",
  "version": "1.0.0",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },
  "permissions": ["activeTab", "declarativeContent"],
  "action": {
    "default_title": "NetSuite Field Explorer",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "24": "img/icon24.png",
      "32": "img/icon32.png"
    }
  },
  "content_security_policy": {}
}
